{
    "name": "omdingmike/laravel-phone-number-validator",
    "description": "This is my package laravel-phone-number-validator",
    "keywords": [
        "OmodingMike",
        "laravel",
        "laravel-phone-number-validator"
    ],
    "homepage": "https://github.com/omdingmike/laravel-phone-number-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "OmodingMike",
            "email": "omodingmike@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "OmodingMike\\PhoneNumberValidator\\": "src/",
            "OmodingMike\\PhoneNumberValidator\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OmodingMike\\PhoneNumberValidator\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OmodingMike\\PhoneNumberValidator\\PhoneNumberValidatorServiceProvider"
            ],
            "aliases": {
                "PhoneNumberValidator": "OmodingMike\\PhoneNumberValidator\\Facades\\PhoneNumberValidator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
